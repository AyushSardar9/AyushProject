pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                bat 'javac Ayush.java'
                echo 'Build in Progress..'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing'
            }
        }
        stage('Deploy') {
            steps {
                bat 'java Ayush'
                echo 'Deploying'
            }
        }
    }
}
